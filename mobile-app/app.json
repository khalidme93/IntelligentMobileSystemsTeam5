{
  "expo": {
    "name": "mobile-app",
    "slug": "mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/mower.png",
    "splash": {
      "image": "src/assets/images/mower.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/mower.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "io.ransomware.com"
    },
    "web": {
      "favicon": "src/assets/image/mower.png"
    }
  }
}
